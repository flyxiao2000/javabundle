<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xcloud.demo.orm.CityMapper">
	<resultMap id="BaseResultMap" type="com.xcloud.demo.domain.City">
		<result column="id" property="id" jdbcType="BIGINT"/>
		<result column="age" property="age" jdbcType="INTEGER"/>
		<result column="city_name" property="name" jdbcType="VARCHAR"/>
	</resultMap>

	<select id="findByID" resultMap="BaseResultMap" parameterType="java.lang.Long">
		select id,age,city_name from city where id = #{id,jdbcType=BIGINT}
	</select>

	<insert id="insert" parameterType="map" useGeneratedKeys="true" keyProperty="userID">
		insert into city (
		id,
		age,
		city_name)
		values (
		#{city.id,jdbcType=BIGINT},
		#{city.age,jdbcType=INTEGER},
		#{city.name,jdbcType=VARCHAR}
		)
	</insert>


</mapper>
